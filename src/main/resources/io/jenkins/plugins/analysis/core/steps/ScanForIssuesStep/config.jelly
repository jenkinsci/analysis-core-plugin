<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core"
         xmlns:f="/lib/form" xmlns:u="/util">

  <f:dropdownDescriptorSelector field="tool" title="${%title.tool}" descriptors="${descriptor.availableTools}"/>
  <f:entry title="${%title.pattern}" field="pattern"
           description="${%description.pattern('http://ant.apache.org/manual/Types/fileset.html')}">
    <f:textbox/>
  </f:entry>
  <u:detectModules/>
  <u:defaultEncoding id="warnings"/>

</j:jelly>
