<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler"
         xmlns:l="/lib/layout"
         xmlns:result="/result">
  <l:layout title="${it.displayName}" norefresh="true">

    <st:include it="${it.owner}" page="sidepanel.jelly" />

    <l:main-panel>

      <h1>${it.header}</h1>

      <result:main />

    </l:main-panel>
  </l:layout>
</j:jelly>